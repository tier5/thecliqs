                    }
            }
        }, {
            keys: [70],
            action: function(a) {
                if (typeof a.enterFullScreen != "undefined")
                    a.isFullScreen ? a.exitFullScreen() : a.enterFullScreen()
                }
        }]
        };
    mejs.mepIndex = 0;
    mejs.players = [];
    mejs.MediaElementPlayer = function(a, b) {
        if (! (this instanceof mejs.MediaElementPlayer))
